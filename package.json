{
  "name": "meeting-room",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.local",
    "generate": "nuxt generate",
    "preview": "npx nuxthub preview",
    "deploy": "npx nuxthub deploy",
    "postinstall": "nuxt prepare",
    "lint": "concurrently 'eslint .' 'stylelint **/*.{vue,html,css,scss,sass}' 'vue-tsc --noEmit'",
    "lint:fix": "concurrently 'eslint . --fix' 'stylelint **/*.{vue,html,css,scss,sass} --fix' 'vue-tsc --noEmit'",
    "format": "prettier . --write",
    "db:generate": "drizzle-kit generate"
  },
  "dependencies": {
