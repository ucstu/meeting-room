    "@nuxtjs/tailwindcss": "7.0.0-beta.0",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
