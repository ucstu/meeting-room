    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.2",
    "lucide-vue-next": "^0.525.0",
    "nuxt": "^3.17.6",
    "nuxt-auth-utils": "0.5.20",
    "nuxt-security": "2.2.0",
    "reka-ui": "^2.3.2",
    "shadcn-nuxt": "^2.2.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.5",
